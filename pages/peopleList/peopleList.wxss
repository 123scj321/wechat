.peopleContainer{
  width:100%;
  display: block;
}
.peopleImages{
  position: relative;
  width:100%;
  height:180rpx;
  overflow: hidden;
}
.peopleBackground{
  position: absolute;
  top:0;
  left:0;
  z-index: -100;
  width:100%;
  /* height:100%; */
}
.peopleAvatar{
  position: absolute;
  top:36rpx;
  left:60rpx;
  width:100rpx;
  height:100rpx;
}
.peopleLogs{
  display: block;
  font-size:32rpx; 
  position: absolute;
  top:80rpx;
  right:200rpx;
  color:#fff;
  /* width:100rpx; */
  height:100rpx;
}
.peopleListContainer{
  margin-top:10rpx;
  width:100%;
  /* background: #fff; */
}
.peopleListContainerRow{
  margin-top:10rpx;
  padding-left: 40rpx;
  /* padding-top: 20rpx; */
  width:100%;
  height:90rpx;
  display: flex;
  flex-flow: row;
  background: #fff;
  line-height: 90rpx;
}
.iconfont{
  width:50rpx;
  height:80rpx;
}
.peopleListContainerRow text{
  font-size: 32rpx;
  color: #666;
}